{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "ngx-data-table": [
        "dist/ngx-data-table"
      ],
      "ngx-data-table/*": [
        "dist/ngx-data-table/*"
      ],
      "ngx-data-form": [
        "dist/ngx-data-form"
      ],
      "ngx-data-form/*": [
        "dist/ngx-data-form/*"
      ]
    }
  }
}